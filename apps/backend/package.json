{
  "name": "@celo-automator/backend",
  "version": "2.0.0",
  "description": "Celo Automator Backend API",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:init": "tsx src/scripts/db-init.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@celo-ai/agents": "workspace:*",
    "@celo-ai/risk-engine": "workspace:*",
    "@celo-automator/celo-functions": "workspace:*",
    "@celo-automator/core": "workspace:*",
    "@celo-automator/langchain-agent": "workspace:*",
    "@celo-automator/types": "workspace:*",
    "@celo-automator/vector-db": "workspace:*",
    "@prisma/client": "^7.0.1",
    "better-sqlite3": "^12.4.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "prisma": "^7.0.1",
    "prom-client": "^15.1.3",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "rimraf": "^6.1.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}

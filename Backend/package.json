  "description": "Advanced AI-powered blockchain automation system with Gemini integration",
  "main": "services/automation-system.js",
  "type": "module",
  "exports": {
    ".": "./services/automation-system.js",
    "./environment": "./services/environment-manager.js",
    "./code-generator": "./services/code-generator.js",
    "./rebalancer": "./services/rebalancer-system.js"
  },

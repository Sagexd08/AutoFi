{
  "name": "@celo-ai/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for the Celo AI agentic automation backend.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "clean": "rimraf dist",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "celo",
    "blockchain",
    "sdk",
    "agents",
    "automation",
    "langchain"
  ],
  "author": "Celo AI Agents Team",
  "license": "MIT",
  "dependencies": {
    "@celo-automator/types": "workspace:*",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

{
  "name": "@celo-automator/celo-functions",
  "version": "2.0.0",
  "type": "module",
  "description": "Modular Celo blockchain function library",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@celo-automator/types": "workspace:*",
    "@celo/contractkit": "^10.0.2",
    "viem": "^2.40.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}

{
  "name": "celo-ai-automation-engine",
  "version": "2.0.0",
  "description": "Advanced AI-powered blockchain automation system with Gemini integration - Complete monorepo with SDK and Backend services",
  "main": "start.js",
  "type": "module",
  "exports": {
    ".": "./start.js",
    "./sdk": "./Backend/sdk/dist/index.js",
    "./autofi": "./Backend/sdk/dist/sdk/autofi-sdk.js",
    "./backend": "./Backend/services/automation-system.js"
  },
  "workspaces": [
    "Backend",
    "Backend/sdk",
    "Frontend",
    "blockchain"
  ],
  "scripts": {
    "start": "node start.js",
    "dev": "node --watch start.js",
    "test": "cd Backend && npm test",
    "test:integration": "cd Backend && npm run test:integration",
    "test:all": "cd Backend && npm run test:all",
    "build": "cd Backend/sdk && npm run build",
    "build:sdk": "cd Backend/sdk && npm run build",
    "lint": "cd Backend && npm run lint",
    "lint:fix": "cd Backend && npm run lint:fix",
    "clean": "cd Backend && npm run clean && cd Backend/sdk && npm run clean",
    "publish:sdk": "cd Backend/sdk && npm publish",
    "publish:backend": "cd Backend && npm publish"
  },
  "keywords": [
    "celo",
    "blockchain",
    "ai",
    "automation",
    "gemini",
    "nft",
    "defi",
    "smart-contracts"
  ],
  "author": "Celo AI Agents Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/celo-ai-agents/automation-engine"
  },
  "bugs": {
    "url": "https://github.com/celo-ai-agents/automation-engine/issues"
  },
  "homepage": "https://github.com/celo-ai-agents/automation-engine#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@supabase/supabase-js": "^2.76.1"
  }
}

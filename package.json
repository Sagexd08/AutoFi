{
  "name": "celo-automator",
  "version": "2.0.0",
  "description": "Advanced AI-powered Celo blockchain automation monorepo with LangChain orchestration",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "turbo run dev",
    "dev:backend": "cd apps/backend && pnpm dev",
    "dev:frontend": "cd Frontend && pnpm dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:coverage": "turbo run test:coverage",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rimraf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "turbo run type-check",
    "deploy": "turbo run deploy",
    "quickstart": "bash scripts/quickstart.sh",
    "quickstart:win": "cmd /c scripts/quickstart.bat",
    "db:generate": "cd apps/backend && pnpm db:generate",
    "db:migrate": "cd apps/backend && pnpm db:migrate",
    "db:init": "cd apps/backend && pnpm db:init",
    "db:studio": "cd apps/backend && pnpm db:studio",
    "health": "curl http://localhost:3001/api/health"
  },
  "keywords": [
    "celo",
    "blockchain",
    "ai",
    "automation",
    "langchain",
    "monorepo",
    "turborepo",
    "defi",
    "smart-contracts",
    "workflow"
  ],
  "author": "Celo AI Agents Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "prettier": "^3.7.1",
    "rimraf": "^6.1.2",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/celo-ai-agents/celo-automator"
  },
  "bugs": {
    "url": "https://github.com/celo-ai-agents/celo-automator/issues"
  },
  "homepage": "https://github.com/celo-ai-agents/celo-automator#readme",
  "dependencies": {
    "@reown/appkit": "^1.6.0",
    "@reown/appkit-adapter-wagmi": "^1.6.0",
    "@tanstack/react-query": "^5.90.11",
    "viem": "^2.40.3",
    "wagmi": "^2.14.16"
  }
}

{
  "name": "@autofi/queue",
  "version": "1.0.0",
  "description": "BullMQ worker queue for Autofi background job processing",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./processors": {
      "import": "./dist/processors/index.js",
      "types": "./dist/processors/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/worker.js",
    "lint": "eslint src/",
    "clean": "rimraf dist .turbo"
  },
  "dependencies": {
    "bullmq": "^5.13.0",
    "ioredis": "^5.4.1",
    "@autofi/database": "workspace:*",
    "@autofi/chain-adapter": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^6.1.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "autofi",
    "queue",
    "bullmq",
    "redis",
    "jobs"
  ],
  "license": "MIT"
}
